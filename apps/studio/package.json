    "expr-eval": "^2.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "manifold-3d": "^3.3.2",
    "next-themes": "^0.4.6",
    "openscad-wasm": "^0.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0"
