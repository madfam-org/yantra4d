  "constraints": [
    {
      "rule": "wall_thickness >= 1.2",
      "message": {
        "es": "Grosor m\u00ednimo para impresi\u00f3n FDM",
        "en": "Minimum thickness for FDM printing"
      },
      "severity": "error",
      "applies_to": [
        "wall_thickness"
      ]
    },
    {
      "rule": "pen_diameter > wall_thickness * 2",
      "message": {
        "es": "El bol\u00edgrafo debe caber en el orificio",
        "en": "Pen must fit in the hole"
      },
      "severity": "error",
      "applies_to": [
        "pen_diameter",
        "wall_thickness"
      ]
    },
    {
      "rule": "phone_angle >= 45",
      "message": {
        "es": "\u00c1ngulo m\u00ednimo de estabilidad",
        "en": "Minimum stability angle"
      },
      "severity": "warning",
      "applies_to": [
        "phone_angle"
      ]
    },
    {
      "rule": "rows * cols <= 9",
      "message": {
        "es": "M\u00e1ximo 9 bandejas en ret\u00edcula",
        "en": "Maximum 9 trays in grid"
