{
    "project": {
        "name": "Custom MSH (AOCL)",
        "slug": "custom-msh",
        "version": "2.0.0",
        "description": {
            "en": "Custom substrate holder system for 1×1 inch substrates (AOCL) — holder, 10-slot rack, and 3-rack storage box",
            "es": "Sistema portamuestras para sustratos de 1×1 pulgada (AOCL) — soporte individual, rack de 10 ranuras y caja para 3 racks"
        },
        "thumbnail": "/projects/custom-msh.webp",
        "tags": [
            "lab",
            "custom",
            "aocl",
            "substrate",
            "microscope",
            "hyperobject"
        ],
        "difficulty": "intermediate"
    },
    "hyperobject": {
        "domain": "medical",
        "vendor": "microscope-slide-holder",
        "vendor_path": "vendor/microscope-slide-holder",
        "cdg_interfaces": [
            {
                "id": "aocl_substrate_pocket",
                "label": {
                    "en": "AOCL 1\"×1\" Substrate Pocket",
                    "es": "Bolsillo para Sustrato AOCL 1\"×1\""
                },
                "geometry_type": "pocket",
                "standard": "internal/aocl",
                "parameters": [
                    "substrate_size",
                    "tolerance_xy",
                    "tolerance_z"
                ]
            },
            {
                "id": "snap_latch_interface",
                "label": {
                    "en": "Snap-Fit Lid Latch",
                    "es": "Pestillo de Tapa a Presión"
                },
                "geometry_type": "snap",
                "standard": "internal",
                "parameters": [
                    "snap_lid"
                ]
            }
        ],
        "commons_license": "CERN-OHL-S-2.0",
        "societal_benefit": {
            "en": "Enables fabrication of a precision substrate retention system for AOCL laboratory workflows — independent of commercial supply chains.",
            "es": "Permite fabricar un sistema de retención de sustratos de precisión para flujos de trabajo de laboratorio AOCL — independiente de cadenas de suministro comerciales."
        }
    },
    "modes": [
        {
            "id": "holder",
            "scad_file": "holder.scad",
            "label": {
                "en": "Single Holder",
                "es": "Soporte Individual"
