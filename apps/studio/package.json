{
  "name": "@yantra4d/studio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "cd ../.. && husky apps/studio/.husky",
    "check-manifest-sync": "diff ../../projects/gridfinity/project.json src/config/fallback-manifest.json || echo 'WARNING: fallback manifest out of sync'",
    "analyze": "ANALYZE=true vite build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "vitest related --run"
    ]
  },
  "dependencies": {
    "@janua/react-sdk": "^0.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
