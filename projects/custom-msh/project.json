            "parts": [
                "holder_body"
            ],
            "estimate": {
                "base_units": 1,
                "formula": "constant"
            }
        },
        {
            "id": "rack",
            "scad_file": "rack.scad",
            "label": {
                "en": "10-Slot Rack",
                "es": "Rack de 10 Ranuras"
            },
            "parts": [
                "rack"
            ],
            "estimate": {
                "base_units": 1,
                "formula": "constant"
            }
        },
        {
            "id": "box",
            "scad_file": "box.scad",
            "label": {
                "en": "3-Rack Box",
                "es": "Caja para 3 Racks"
            },
            "parts": [
                "box_base",
                "box_lid"
            ],
            "estimate": {
                "base_units": 1,
                "formula": "constant"
            }
        }
